<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.core.vipcareplus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:theme="@style/AppTheme"
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".abuselog"
            android:theme="@style/AbuseFormTheme"
            android:parentActivityName="com.core.vipcareplus.MainActivity"
            android:label="Abuse Log"></activity>
        <activity android:name=".abuseform"
            android:parentActivityName="com.core.vipcareplus.abuselog"
            android:label="Report New Abuse"
            android:theme="@style/AbuseFormTheme"></activity>
    </application>

</manifest>