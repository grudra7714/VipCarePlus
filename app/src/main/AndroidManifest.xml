<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.core.vipcareplus">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".abuselog"
            android:label="ABUSE LOG"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AbuseFormTheme" />

        <activity
            android:name=".abuseform"
            android:label="REPORT NEW ABUSE"
            android:parentActivityName=".abuselog"
            android:theme="@style/AbuseFormTheme" />
        <activity
            android:name=".emergency"
            android:label="VIP CARE + ALERT"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />

        <activity android:name=".complete_information"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme"/>

        <activity android:name=".introduction"
            android:parentActivityName=".complete_information"
            android:theme="@style/AppTheme"    />

        <activity android:name=".attack"
            android:parentActivityName=".complete_information"
            android:theme="@style/AppTheme"/>

        <activity android:name=".neglect"
            android:parentActivityName=".complete_information"
            android:theme="@style/AppTheme"/>

        <activity android:name=".stealing"
            android:parentActivityName=".complete_information"
            android:theme="@style/AppTheme"/>

        <activity android:name=".warning"
            android:parentActivityName=".complete_information"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>